package uk.co.richtextformat.utils {	/**	 * @author richardwillis	 */	public class Statics	{		static public function report ( t:* ):void		{			var d:Date = new Date();			trace( "["+d.getHours() + ":" +d.getMinutes() + ":" +d.getSeconds() + ":" +d.getMilliseconds() + "] : " + t );		}				static public function camelCase ( strings:Array, forceToLowerCase:Boolean = true ):String		{			var s:String = strings[0].toString().toLowerCase();			var i:int = 0;			var len:int = strings.length;			while (++i<len){				s += capitalise( strings[i], forceToLowerCase );			}						return s;		}				static public function capitalise ( string:String, forceToLowerCase:Boolean = true ):String		{			var newString:String = string.substring(0,1).toUpperCase();			var suffix:String = string.substring(1, string.length)			newString += forceToLowerCase? suffix.toLowerCase() : suffix;			return newString;		}	}}